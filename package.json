{
  "name": "setup-poetry",
  "version": "0.0.0",
  "private": true,
  "description": "Set up your GitHub Actions workflow with a specific version of Poetry",
  "main": "lib/main.js",
  "scripts": {
    "build:compile": "tsc",
    "build:dist": "ncc build",
    "build": "npm-run-all build:*",
    "lint:eslint": "eslint src/**/*.ts",
    "lint:prettier": "prettier --check {*.json,*.yml,**/*.ts,.github/**/*.yml}",
    "lint": "npm-run-all lint:*",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gr1N/setup-poetry.git"
  },
  "keywords": [
    "actions",
    "setup",
    "python",
    "poetry"
  ],
  "author": "Nikita Grishko",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.2",
    "@actions/tool-cache": "^1.1.2",
    "@types/semver": "^6.2.0",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^26.2.0",
    "typescript": "^3.6.4"
  }
}
