{
  "name": "setup-poetry",
  "version": "0.0.0",
  "private": true,
  "description": "Set up your GitHub Actions workflow with a specific version of Poetry",
  "main": "lib/main.js",
  "scripts": {
    "build:compile": "tsc",
    "build:dist": "ncc build",
    "build": "npm-run-all build:*",
    "lint:eslint": "eslint src/**/*.ts",
    "lint:prettier": "prettier --check {*.json,*.yml,**/*.ts,.github/**/*.yml}",
    "lint": "npm-run-all lint:*",
    "test": "jest --coverage",
    "ts-node": "ts-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gr1N/setup-poetry.git"
  },
  "keywords": [
    "actions",
    "setup",
    "python",
    "poetry"
  ],
  "author": "Nikita Grishko",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.0",
    "@actions/tool-cache": "^1.7.1",
    "@types/semver": "^7.3.6",
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@typescript-eslint/parser": "^4.28.1",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.29.0",
    "eslint-plugin-github": "^4.0.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.0",
    "jest-circus": "^27.0.0",
    "js-yaml": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
